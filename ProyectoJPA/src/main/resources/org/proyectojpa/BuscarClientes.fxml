<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          xmlns="http://javafx.com/javafx"
          fx:controller="org.proyectojpa.controller.BuscarClientesController"
          prefHeight="400.0" prefWidth="600.0">
    
    <Label text="ID" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="idCliente" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <Label text="DNI" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="dniCliente" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label text="Nombre" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <TextField fx:id="nombreCliente" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Label text="Email" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <TextField fx:id="emailCliente" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
</GridPane>