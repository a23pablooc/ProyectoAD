<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          xmlns="http://javafx.com/javafx"
          fx:controller="org.proyectojpa.controller.NuevoLibroController"
          prefHeight="400.0" prefWidth="600.0">

    <Label text="Código" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="codigo" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <Label text="Título" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="titulo" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label text="Autores" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <TextField fx:id="autores" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Label text="Año" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <TextField fx:id="anho" GridPane.columnIndex="1" GridPane.rowIndex="3"/>

</GridPane>
